{
  "name": "oleksiikachan.com",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cross-env jest --no-cache --passWithNoTests",
    "prepare": "husky install",
    "pre-commit": "npm-run-all -p pre-commit:*",
    "pre-commit:lint": "yarn lint --fix && yarn pretty-quick --staged",
    "pre-commit:test": "cross-env jest --onlyChanged"
  },
  "engines": {
    "node": "20"
  },
  "jest-junit": {
    "output": "./test-report.xml"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {
    "@sanity/client": "^6.10.0",
    "@sanity/icons": "^2.0",
    "@sanity/image-url": "1",
    "@sanity/types": "^3.19",
    "@sanity/ui": "^2.0.0-beta.14",
    "@sanity/vision": "3",
    "@sentry/nextjs": "^7.92.0",
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "framer-motion": "^10.17.9",
    "logrocket": "^7.0.0",
    "logrocket-react": "^6.0.3",
    "next": "^14.0",
    "next-sanity": "7.0.6",
    "react": "^18.2",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "react-markdown": "^9.0.1",
    "sanity": "^3.19",
