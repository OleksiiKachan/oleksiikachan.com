{
  "name": "oleksiikachan.com",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cross-env jest --no-cache --passWithNoTests",
    "prepare": "husky install",
    "pre-commit": "npm-run-all -p pre-commit:*",
    "pre-commit:lint": "yarn lint --fix && yarn pretty-quick --staged",
    "pre-commit:test": "cross-env jest --onlyChanged"
  },
  "engines": {
    "node": "18"
  },
  "jest-junit": {
    "output": "./test-report.xml"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {
    "@sentry/nextjs": "^7.51.2",
    "@vercel/analytics": "^1.0.1",
    "framer-motion": "^10.12.10",
    "logrocket": "^4.0.1",
    "logrocket-react": "^5.0.1",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "react-markdown": "^8.0.7",
    "sharp": "^0.32.1",
    "styled-components": "^5.3.10",
    "wonder-engine": "^0.0.36"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/logrocket-react": "^3.0.0",
    "@types/node": "20.1.2",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "typescript": "5.0.4"
  }
}
