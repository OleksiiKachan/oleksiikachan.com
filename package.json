{
  "name": "oleksiikachan.com",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cross-env jest --no-cache --passWithNoTests",
    "prepare": "husky install",
    "pre-commit": "npm-run-all -p pre-commit:*",
    "pre-commit:lint": "yarn lint --fix && yarn pretty-quick --staged",
    "pre-commit:test": "cross-env jest --onlyChanged"
  },
  "engines": {
    "node": "18"
  },
  "jest-junit": {
    "output": "./test-report.xml"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {
    "@sentry/nextjs": "^7.81.0",
    "@vercel/analytics": "^1.1.1",
    "framer-motion": "^10.16.5",
    "logrocket": "^7.0.0",
    "logrocket-react": "^6.0.1",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "react-markdown": "^9.0.1",
    "sharp": "^0.32.6",
    "styled-components": "^6.1.1",
    "wonder-engine": "^0.0.36"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/logrocket-react": "^3.0.3",
    "@types/node": "20.9.2",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/styled-components": "^5.1.30",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "typescript": "5.2.2"
  }
}
