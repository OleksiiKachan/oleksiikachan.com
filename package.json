    "framer-motion": "^12.23.13",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "next": "15.5.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-is": "^19.1.1",
    "react-markdown": "^10.1.0",
    "sharp": "^0.34.3",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
