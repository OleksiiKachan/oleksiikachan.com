    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {
    "@sentry/nextjs": "^10.12.0",
    "@vercel/analytics": "^1.5.0",
