{
  "name": "oleksiikachan.com",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test:unit": "cross-env jest --no-cache --passWithNoTests",
    "check-types": "tsc --noEmit",
    "pre-commit": "npm-run-all -p pre-commit:*",
    "pre-commit:lint": "yarn lint --fix && yarn pretty-quick --staged",
    "pre-commit:test": "cross-env jest --onlyChanged"
  },
  "engines": {
    "node": "22"
  },
  "jest-junit": {
    "output": "./test-report.xml"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".commitizen.json"
    }
  },
  "dependencies": {
    "@sentry/nextjs": "^10.22.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "framer-motion": "^12.23.24",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/logrocket-react": "^3.0.3",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "cross-env": "^10.1.0",
    "cz-customizable": "^7.5.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "sass": "^1.93.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "resolutions": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
